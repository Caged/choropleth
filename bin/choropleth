#!/usr/bin/env node
var choropleth = require('../'),
    fs = require('fs')

var argv = require('yargs')
  .default('projection', 'albersUsa')
  .default('width', 960)
  .default('height', 600)
  .default('out', '/dev/stdout')
  .argv

argv.out = argv.out === '/dev/stdout'
  ? process.stdout
  : fs.createWriteStream(argv.out, "utf8")

argv.input = argv._

choropleth(argv)
